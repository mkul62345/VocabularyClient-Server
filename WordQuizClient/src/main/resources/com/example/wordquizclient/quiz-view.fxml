<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox nodeOrientation="LEFT_TO_RIGHT" prefHeight="492.0" prefWidth="644.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wordquizclient.QuizController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>



  <Label fx:id="wordDescription" text="Question " />
   <RadioButton fx:id="radio1" mnemonicParsing="false" text="Option 1"/>
   <RadioButton fx:id="radio2" mnemonicParsing="false" text="Option 2"/>
   <RadioButton fx:id="radio3" mnemonicParsing="false" text="Option 3"/>
   <RadioButton fx:id="radio4" mnemonicParsing="false" text="Option 4"/>
   <RadioButton fx:id="radio5" mnemonicParsing="false" text="Option 5"/>
  <Button onAction="#onSubmitButtonClick" prefHeight="25.0" prefWidth="78.0" text="Submit" />
</VBox>
